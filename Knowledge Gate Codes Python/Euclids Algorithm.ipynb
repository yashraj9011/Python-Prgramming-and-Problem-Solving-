   "source": [
    "a = int(input())\n",
    "b = int(input())\n",
    "if a>b:\n",
    "    l=a\n",
    "else:\n",
    "    l=b\n",
    "for i in range(l,(a*b)+1,l):\n",
    "    if(i%a==0 and i%b==0):\n",
    "        break\n",
    "lcm=i\n",
    "hcf=(a*b)//lcm\n",
    "print(\"LCM is: \",lcm)\n",
    "print(\"HCF is: \",hcf)"
   ]
